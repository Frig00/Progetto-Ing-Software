<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import org.kordamp.ikonli.javafx.*?>

<!--<?import it.unipv.po.aioobe.trenissimo.view.ViaggioControl?>-->

<BorderPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unipv.po.aioobe.trenissimo.controller.RicercaController">
   <center>
      <ScrollPane fitToWidth="true" hbarPolicy="NEVER" prefHeight="448.0" prefWidth="629.0" vbarPolicy="ALWAYS">
          <content>
              <VBox fx:id="layout" alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" spacing="8.0">
               <padding>
                  <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
               </padding>
            </VBox>
          </content>
      </ScrollPane>
   </center>
   <top>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <Label onMouseClicked="#onGoToHomepage" text="Torna a homepage">
               <graphic>
                  <FontIcon iconLiteral="fas-arrow-left" />
               </graphic>
               <font>
                  <Font size="18.0" />
               </font>
               <padding>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </padding>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Label>
         </children>
      </HBox>
   </top>
</BorderPane>
