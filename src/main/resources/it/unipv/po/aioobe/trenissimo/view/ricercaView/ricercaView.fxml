<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import org.kordamp.ikonli.javafx.*?>

<!--<?import it.unipv.po.aioobe.trenissimo.view.ViaggioControl?>-->

<BorderPane prefHeight="660.0" prefWidth="992.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unipv.po.aioobe.trenissimo.controller.RicercaController">
    <center>
        <VBox>
            <children>
            <TabPane fx:id="tabPane" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
               <tabs>
               </tabs>
            </TabPane>
            </children>
        </VBox>
    </center>
    <top>
        <HBox BorderPane.alignment="CENTER">
            <children>
                <Label onMouseClicked="#onGoToHomepage" text="Torna a homepage">
                    <graphic>
                        <FontIcon iconLiteral="fas-arrow-left" />
                    </graphic>
                    <font>
                        <Font size="18.0" />
                    </font>
                    <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                    </padding>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </Label>
            </children>
        </HBox>
    </top>
   <bottom>
          <HBox alignment="CENTER_LEFT" prefWidth="599.0" BorderPane.alignment="CENTER">
              <children>
                  <ScrollPane vbarPolicy="NEVER" HBox.hgrow="ALWAYS">
                      <content>
                          <HBox fx:id="boxCart" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity" prefHeight="136.0" spacing="8.0">
                              <padding>
                                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                              </padding>
                          </HBox>
                      </content>
                  </ScrollPane>
                  <Button minWidth="-Infinity" mnemonicParsing="false" text="Procedi con l'acquisto" HBox.hgrow="NEVER">
                      <HBox.margin>
                          <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                      </HBox.margin>
                  </Button>
              </children>
          </HBox>
   </bottom>
</BorderPane>
